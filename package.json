{
  "name": "react-router-typed",
  "version": "1.0.0",
  "homepage": "https://github.com/jaredLunde/react-router-typed#readme",
  "repository": "github:jaredLunde/react-router-typed",
  "bugs": "https://github.com/jaredLunde/react-router-typed/issues",
  "license": "MIT",
  "description": "A TypeScript implementation of react-router v6 with strong types",
  "keywords": [
    "react",
    "routing",
    "router",
    "react router",
    "react router typescript",
    "react router types",
    "react router dom",
    "react router dom types",
    "react router dom typescript"
  ],
  "main": "dist/main/index.js",
  "module": "dist/module/index.js",
  "source": "src/index.ts",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/module/index.js",
      "import": "./dist/module/index.js",
      "require": "./dist/main/index.js"
    },
    "./dom": {
      "browser": "./dom/dist/module/index.js",
      "import": "./dom/dist/module/index.js",
      "require": "./dom/dist/main/index.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "files": [
    "/src",
    "/dist",
    "/dom/src",
    "/dom/dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run build-main && npm run build-module && npm run build-types",
    "build-main": "npm run compile -- -d dist/main --env-name main && npm run compile-dom -- -d dom/dist/main --env-name main",
    "build-module": "npm run compile -- -d dist/module --env-name module && npm run compile-dom -- -d dom/dist/module --env-name module",
    "build-types": "find src -name '*.d.ts' -exec rm {} \\; && find dom/src -name '*.d.ts' -exec rm {} \\; && tsc -p tsconfig.json -d --emitDeclarationOnly",
    "check-types": "tsc --noEmit -p tsconfig.json",
    "compile": "babel src -x .ts,.tsx --ignore \"**/*.d.ts\",\"**/*.test.ts\",\"**/test.ts\",\"**/*.test.tsx\",\"**/test.tsx\" --delete-dir-on-start",
    "compile-dom": "babel dom/src -x .ts,.tsx --ignore \"**/*.d.ts\",\"**/*.test.ts\",\"**/test.ts\",\"**/*.test.tsx\",\"**/test.tsx\" --delete-dir-on-start",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,yml,json,eslintrc,prettierrc}\"",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublishOnly": "npm run lint && npm run test && npm run build && npm run format",
    "test": "jest",
    "validate": "npm run check-types && npm run lint && npm run test -- --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build-types && git add types && lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint",
      "prettier --write"
    ],
    "**/*.{md,yml,json,eslintrc,prettierrc}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "latest",
    "@lunde/babel-preset-es": "latest",
    "@testing-library/jest-dom": "latest",
    "@testing-library/react": "latest",
    "@testing-library/react-hooks": "latest",
    "@types/jest": "latest",
    "@types/node": "^13.11.1",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "babel-plugin-optimize-react": "latest",
    "babel-plugin-typescript-to-proptypes": "latest",
    "eslint": "latest",
    "eslint-import-resolver-jest": "latest",
    "eslint-plugin-jest": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "husky": "latest",
    "jest": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-test-renderer": "latest",
    "ts-jest": "latest",
    "typescript": "latest"
  },
  "dependencies": {
    "create-async-component": "^1.0.4",
    "create-async-route": "^1.1.3",
    "history": "^5.0.0-beta.8"
  },
  "peerDependencies": {
    "prop-types": ">=15.6",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  }
}
